{
    "name": "mblet-syntax-cpp",
    "displayName": "mblet-syntax-cpp",
    "description": "Add simply syntax for C++ language",
    "version": "0.1.0",
    "publisher": "mblet",
    "engines": {
        "vscode": "^0.0.0"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MickaelBlet/syntax-cpp.git"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "id": "Default Dark+ mblet",
                "label": "Dark+ (default dark) (mblet)",
                "uiTheme": "vs-dark",
                "path": "./themes/dark_plus_mblet.json"
            }
        ],
        "languages": [
            {
                "id": "c",
                "extensions": [
                    ".c"
                ],
                "aliases": [
                    "C",
                    "c"
                ]
            },
            {
                "id": "cpp",
                "extensions": [
                    ".cpp",
                    ".cc",
                    ".cxx",
                    ".hpp",
                    ".hh",
                    ".hxx",
                    ".h",
                    ".ino",
                    ".inl",
                    ".ipp"
                ],
                "aliases": [
                    "C++",
                    "Cpp",
                    "cpp"
                ]
            }
        ],
        "grammars": [
            {
                "language": "c",
                "scopeName": "mblet.source.cpp",
                "path": "./syntaxes/cpp.tmLanguage.json"
            },
            {
                "language": "cpp",
                "scopeName": "mblet.source.cpp",
                "path": "./syntaxes/cpp.tmLanguage.json"
            }
        ]
    }
}